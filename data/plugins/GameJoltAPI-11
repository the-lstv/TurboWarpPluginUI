function gja_el(id){return document.getElementById(id);}
async function delayScratchBlock(m){await awaituntill(m);}
function awaituntill(ms){return new Promise(resolve => setTimeout(resolve, ms))}
var GJAPI={},GJUserdata_me={},GJUserdata_fetch={},gjapi_fetch_user_finished=false,gjapi_fetch_user_result;const icon="data:image/png;base64,";
